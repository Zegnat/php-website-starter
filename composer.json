{
    "name": "zegnat/website-starter",
    "description": "My minimum viable setup for starting a PHP project.",
    "type": "project",
    "version": "4.0.0",
    "license": "0BSD",
    "authors": [
        {
            "name": "Martijn van der Ven",
            "email": "martijn@vanderven.se"
        }
    ],
    "autoload": {
        "psr-4": {"app\\": "app/"}
    },
    "require": {
        "http-interop/http-factory-diactoros": "^1.0",
        "middlewares/fast-route": "dev-master#c82a003912e7aff629243c4a75fb1b5d55e372ba",
        "nyholm/psr7-server": "^0.3.0",
        "oscarotero/middleland": "^1.0",
        "rdlowrey/auryn": "^1.4",
        "zendframework/zend-diactoros": "^1.8",
        "zendframework/zend-httphandlerrunner": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    },
    "scripts": {
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 app config public"
    },
    "config": {
        "sort-packages": true
    }
}
